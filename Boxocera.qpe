{
  "js": "class xor {\n    static encode(str) {\n        return encodeURIComponent(\n            str\n                .toString()\n                .split(\"\")\n                .map((char, ind) =>\n                    ind % 2 ? String.fromCharCode(char.charCodeAt() ^ 2) : char\n                )\n                .join(\"\")\n        );\n    }\n    static decode(str) {\n        if (str.charAt(str.length - 1) == \"/\") str = str.slice(0, -1);\n        return decodeURIComponent(str)\n            .split(\"\")\n            .map((char, ind) =>\n                ind % 2 ? String.fromCharCode(char.charCodeAt() ^ 2) : char\n            )\n            .join(\"\");\n    }\n}\n\nlet activeGame = \"gavin\";\nfunction moveSelected(dir) {\n    const left = document.querySelector('img.l');\n    const mid = document.querySelector('img.s');\n    const right = document.querySelector('img.r');\n    var leftSrc = left.src;\n    var midSrc = mid.src;\n    var rightSrc = right.src;\n    if(dir == \"right\") {\n        right.src = leftSrc;\n        mid.src = rightSrc;\n        left.src = midSrc;\n    }else if(dir == \"left\") {\n        right.src = midSrc;\n        mid.src = leftSrc;\n        left.src = rightSrc;\n    }\n    activeGame = mid.src.split(\"https://github.com/GavinGoGaming/boxocera/blob/main/\")[1].split('Logo.png')[0].toLowerCase();\n}\n\ndocument.addEventListener('DOMContentLoaded', () => {\n    const duration = 1500;\n    var curListener = null;\n    var curOtherListener = null;\n    var warn = document.getElementsByClassName('warn')[0];\n\n    var menu = document.querySelector('.menu');\n    var game = document.querySelector('.game');\n\n    function show(e) {\n        e.preventDefault();\n        if (curListener != null) {\n            clearTimeout(curListener);\n            clearTimeout(curOtherListener);\n        }\n        warn.classList.add('active');\n        curListener = setTimeout(() => {\n            warn.classList.remove('active');\n            warn.classList.add('disabling');\n            curOtherListener = setTimeout(() => {\n                warn.className = \"warn\";\n            }, 1000 * 0.25);\n        }, duration)\n    }\n\n    var showWarn = show;\n\n    document.querySelector('body').addEventListener('click', showWarn);\n    document.querySelector('body').addEventListener('contextmenu', showWarn);\n\n    const gamesBox = [{ \"name\": \"Box\", \"id\": 768067716, \"author\": { \"id\": 78538507, \"name\": \"Bluurii\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538507_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/768067716_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/768067716\" }, { \"name\": \"The Box II\", \"id\": 779794593, \"author\": { \"id\": 78538507, \"name\": \"Bluurii\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538507_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/779794593_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/779794593\" }, { \"name\": \"Box remix\", \"id\": 896253667, \"author\": { \"id\": 96511432, \"name\": \"wesjanson\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/96511432_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/896253667_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/896253667\" }, { \"name\": \"the box 3\", \"id\": 811209008, \"author\": { \"id\": 96511432, \"name\": \"wesjanson\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/96511432_90x90.png\", \"url\": \"https://scratch.mit.edu/projects/918235963\" } }, { \"name\": \"box world tour\", \"id\": 903233620, \"author\": { \"id\": 96511432, \"name\": \"wesjanson\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/96511432_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/903233620_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/903233620\" }, { \"name\": \"cave story (box 5)\", \"id\": 918235963, \"author\": { \"id\": 96511432, \"name\": \"wesjanson\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/96511432_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/918235963_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/918235963\" }, { \"name\": \"a box to the past (box 7)\", \"id\": 924608045, \"author\": { \"id\": 96511432, \"name\": \"wesjanson\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/96511432_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/924608045_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/924608045\" }, { \"name\": \"THE BOX VIII\", \"id\": 1061257313, \"author\": { \"id\": 78538507, \"name\": \"Bluurii\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538507_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/1061257313_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/1061257313\" }, { \"name\": \"Bluurii Battle Royale\", \"id\": 1043170192, \"author\": { \"id\": 78538507, \"name\": \"Bluurii\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538507_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/1043170192_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/1043170192\" }];\n    const gamesShkr = [{ \"name\": \"Shkr generator\", \"id\": 1023124126, \"author\": { \"id\": 78538507, \"name\": \"Bluurii\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538507_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/1023124126_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/1023124126\" }, { \"name\": \"Shkrmishshkr Adventures Continues\", \"id\": 780283039, \"author\": { \"id\": 78538507, \"name\": \"Bluurii\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538507_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/780283039_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/780283039\" }, { \"name\": \"Shkrmishshkr parkour\", \"id\": 836781717, \"author\": { \"id\": 97709060, \"name\": \"whatislifelike\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/97709060_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/836781717_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/836781717\" }, { \"name\": \"shkrmishshkr the legendary racer\", \"id\": 712794173, \"author\": { \"id\": 78538474, \"name\": \"AndrewKwak23\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538474_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/712794173_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/712794173\" }, { \"name\": \"Shkrmishshkr ATTACK!!!!!!!!!!!!!!!\", \"id\": 753518637, \"author\": { \"id\": 78538507, \"name\": \"Bluurii\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538507_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/753518637_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/753518637\" }, { \"name\": \"Shkrmishshkr click trainer\", \"id\": 712751794, \"author\": { \"id\": 78538507, \"name\": \"Bluurii\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538507_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/712751794_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/712751794\" }, { \"name\": \"Shkrmish fighter (BETA)\", \"id\": 712774032, \"author\": { \"id\": 78538507, \"name\": \"Bluurii\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538507_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/712774032_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/712774032\" }, { \"name\": \"Shkrmishshkr Adventures: The sequel!\", \"id\": 545285416, \"author\": { \"id\": 78538507, \"name\": \"Bluurii\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538507_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/545285416_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/545285416\" }, { \"name\": \"SUPER TANK\", \"id\": 548019780, \"author\": { \"id\": 78538507, \"name\": \"Bluurii\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538507_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/548019780_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/548019780\" }, { \"name\": \"shkrmishshkr rpg\", \"id\": 706135608, \"author\": { \"id\": 78538474, \"name\": \"AndrewKwak23\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538474_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/706135608_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/706135608\" }, { \"name\": \"the attack of ooga booga (shkrmishshkr game)\", \"id\": 670049305, \"author\": { \"id\": 78538474, \"name\": \"AndrewKwak23\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538474_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/670049305_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/670049305\" }, { \"name\": \"Shkrmishshkr's adventure\", \"id\": 544862114, \"author\": { \"id\": 78538474, \"name\": \"AndrewKwak23\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538474_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/544862114_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/544862114\" }, { \"name\": \"shkrmishshkr's revenge\", \"id\": 545379372, \"author\": { \"id\": 78538474, \"name\": \"AndrewKwak23\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538474_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/545379372_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/545379372\" }, { \"name\": \"shkrmishshkr clicker!\", \"id\": 663599932, \"author\": { \"id\": 78538474, \"name\": \"AndrewKwak23\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538474_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/663599932_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/663599932\" }, { \"name\": \"shkrmishshkr's supa laggy game\", \"id\": 550263872, \"author\": { \"id\": 78538474, \"name\": \"AndrewKwak23\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538474_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/550263872_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/550263872\" }, { \"name\": \"shkrmishshkr grass eating simulator\", \"id\": 650712403, \"author\": { \"id\": 78538474, \"name\": \"AndrewKwak23\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/78538474_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/650712403_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/650712403\" }];\n    const gamesGavin = [{ \"name\": \"FINGERBOARD WALL\", \"id\": 927243782, \"author\": { \"id\": 58813447, \"name\": \"GavinGoGaming\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/58813447_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/927243782_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/927243782/\" }, { \"name\": \"Just Blocks & Notes [v1.2]\", \"id\": 1036343147, \"author\": { \"id\": 58813447, \"name\": \"GavinGoGaming\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/58813447_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/1036343147_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/1036343147\" }, { \"name\": \"TAG (Beta)\", \"id\": 909260519, \"author\": { \"id\": 58813447, \"name\": \"GavinGoGaming\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/58813447_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/909260519_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/909260519\" }, { \"name\": \"Goobmetry Craft (1:1 Minecraft)\", \"id\": 1043374840, \"author\": { \"id\": 58813447, \"name\": \"GavinGoGaming\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/58813447_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/1043374840_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/1043374840/\" }, { \"name\": \"chop n' go - Rule a Kingdom\", \"id\": 937072983, \"author\": { \"id\": 58813447, \"name\": \"GavinGoGaming\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/58813447_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/937072983_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/937072983\" }, { \"name\": \"two birds one stone | a puzzle game (V1.7)\", \"id\": 921577616, \"author\": { \"id\": 58813447, \"name\": \"GavinGoGaming\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/58813447_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/921577616_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/921577616\" }, { \"name\": \"Geometry Dash E12 (with Skins, Hacks, and Colors)\", \"id\": 947632035, \"author\": { \"id\": 58813447, \"name\": \"GavinGoGaming\", \"image\": \"https://cdn2.scratch.mit.edu/get_image/user/58813447_90x90.png?v=\" }, \"image\": \"https://cdn2.scratch.mit.edu/get_image/project/947632035_480x360.png\", \"url\": \"https://scratch.mit.edu/projects/947632035\" }];\n    var games = {\n        \"box\": gamesBox,\n        \"shkr\": gamesShkr,\n        \"gavin\": gamesGavin\n    };\n    const validKeys = [\"ArrowLeft\", \"ArrowUp\", \"ArrowDown\", \"ArrowRight\", \"Enter\", \" \", \"Escape\"];\n    var selectedGame = 0;\n\n    var opened = false;\n\n    document.querySelector('body').addEventListener('keydown', (ev) => {\n        if (validKeys.includes(ev.key)) {\n            if (opened) return;\n\n            if(ev.key == \"ArrowDown\" || ev.key == \"Space\" || ev.key == \"Enter\") {\n                opened = true;\n                showGame();\n            }\n            if (ev.key == \"ArrowLeft\") { moveSelected('left'); }\n            if (ev.key == \"ArrowRight\") { moveSelected('right'); }\n        }\n    })\n\n    var editor = document.querySelector('.editor');\n    var curGameEmbed = \"\", curGameLink = \"\";\n    document.querySelector('.close').addEventListener('click', () => {\n        editor.classList.add('hidden');\n        editorFrame.src = \"\";\n    })\n    document.querySelector('#big').addEventListener('click', () => {\n        editor.classList.remove('hidden');\n        editorFrame.src = curGameEmbed;\n    })\n    document.querySelector('#closepage').addEventListener('click', () => {\n        window.close();\n    })\n    document.querySelector('#link').addEventListener('click', () => {\n        window.open(curGameLink);\n    })\n    function showGame() {\n        games[activeGame].map((game, idx) => {\n            const btn = document.createElement('button');\n            btn.textContent = game.name;\n            btn.addEventListener('click', () => {\n                selectedGame = idx;\n                loadGame(games[activeGame][selectedGame]);\n            })\n            document.querySelector('.games').append(btn);\n        })\n        game.classList.remove('hidden');\n        menu.classList.add('hidden');\n        showWarn = function () { };\n        loadGame(games[activeGame][selectedGame]);\n    }\n\n    function is(id) {\n        return document.querySelector('#' + id).classList.contains('checked');\n    }\n\n    const url = \"https://not.counterfeitcontent.com/uv/service/\";\n\n    var gameFrame = document.querySelector('iframe');\n    var gameAuthorImage = document.querySelector('#gameAuthorImage');\n    var gameAuthor = document.querySelector('#gameAuthor');\n    var gameName = document.querySelector('#gameName');\n    var gameThumb = document.querySelector('#gameThumb');\n    var editorFrame = document.querySelector('#editorFrame');\n    function loadGame(game) {\n        var turbowarpLink = \"https://turbowarp.org/\" + game.id + \"/embed\" + `?fullscreen-button&username=BoxoceraUser${is('interpolate') && \"&interpolate\"}`;\n        gameFrame.src = turbowarpLink;\n        gameAuthorImage.src = game.author.image;\n        gameAuthor.textContent = game.author.name;\n        gameName.textContent = game.name;\n        gameThumb.classList.remove('hidden');\n        gameThumb.src = game.image;\n        gameFrame.addEventListener('load', () => {\n            setTimeout(() => gameThumb.classList.add('hidden'), 600);\n        });\n        curGameEmbed = turbowarpLink;\n        curGameLink = game.url;\n    }\n    function updateGame() {\n        gameFrame.src = gameFrame.src;\n    }\n\n    // showGame();\n\n    document.querySelectorAll('.checkbox').forEach(box => {\n        box.addEventListener('click', () => {\n            box.classList.toggle('checked');\n            updateGame();\n        })\n    })\n    document.querySelector('#reload').addEventListener('click', () => {\n        updateGame();\n    });\n})",
  "css": "@import url(\"https://rsms.me/inter/inter.css\");\n@import url(\"https://site-assets.fontawesome.com/releases/v6.6.0/css/all.css\");\n@import url(\"https://site-assets.fontawesome.com/releases/v6.6.0/css/sharp-solid.css\");\n\n:root {\n    --border: 4px solid #3066B3;\n    --hot: #D1D0D1;\n    --cold: #7D7C7C;\n}\n\nhtml,\nbody {\n    background: #060C16;\n    color: var(--hot);\n    font-family: Inter;\n    padding: 0;\n    margin: 0;\n    display: flex;\n    width: 100vw;\n    height: 100vh;\n    cursor: crosshair;\n}\n\n.bar {\n    background: #010205;\n    border-top: 4px solid #325CAC;\n    height: 50px;\n    position: absolute;\n    bottom: 0;\n    width: calc(100vw - 12px);\n    display: flex;\n    flex-direction: row;\n    padding-left: 12px;\n    align-items: center;\n}\n\n.center {\n    height: calc(100vh - 54px);\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n}\n\n.mid {\n    height: 180px;\n    width: 100vw;\n    position: relative;\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    border-top: var(--border);\n    border-bottom: var(--border);\n    background-color: rgba(200, 200, 255, 0.04);\n}\n\n.mid::before {\n    width: 100vw;\n    height: 40px;\n    content: \"\";\n    position: absolute;\n    background: linear-gradient(transparent, #17253f);\n    top: -44px;\n}\n\n.mid::after {\n    width: 100vw;\n    height: 40px;\n    content: \"\";\n    position: absolute;\n    background: linear-gradient(#17253f, transparent);\n    bottom: -44px;\n}\n\n#top-left {\n    top: 8px;\n    left: 8px;\n    position: absolute;\n    font-size: 22px;\n}\n\n#top-left-2 {\n    top: 38px;\n    left: 10px;\n    font-size: 14px;\n    position: absolute;\n}\n\n.mid {\n    justify-content: space-around;\n}\n\n.r,\n.l {\n    max-width: 20%;\n    filter: blur(1.5px) grayscale(0.3) opacity(0.85);\n}\n\n.s {\n    height: 220px;\n}\n\n.pre,\n.post {\n    position: absolute;\n    left: 0;\n    height: 100%;\n    width: 170px;\n}\n\n.pre {\n    background: linear-gradient(to right, #17253f80, transparent);\n}\n\n.post {\n    right: 0;\n    left: unset;\n    background: linear-gradient(to left, #17253f80, transparent);\n}\n\n* {\n    user-select: none;\n}\n\n.warn {\n    position: absolute;\n    background-color: #00000050;\n    width: 100vw;\n    height: 100vh;\n    backdrop-filter: blur(10px);\n    top: 0;\n    left: 0;\n    flex-direction: column;\n    display: none;\n    gap: 2px;\n    align-items: center;\n    justify-content: center;\n    z-index: 5;\n}\n\n.warn h1 {\n    margin: 2px 0;\n}\n\n#icons {\n    gap: 2px;\n    display: flex;\n}\n\n#icons i {\n    font-size: 25px;\n}\n\n@keyframes warnShow {\n    0% {\n        opacity: 0;\n        display: flex;\n    }\n\n    100% {\n        opacity: 1;\n        display: flex;\n    }\n}\n\n@keyframes warnHide {\n    0% {\n        opacity: 1;\n        display: flex;\n    }\n\n    100% {\n        opacity: 0;\n        display: none;\n    }\n}\n\n.warn.active {\n    display: flex;\n    animation: warnShow 0.2s ease forwards;\n}\n\n.warn.disabling {\n    animation: warnHide 0.25s ease forwards !important;\n}\n\n.bar {\n    gap: 15px;\n}\n\n.bar #icon i {\n    color: #3066B3;\n}\n\n.bar #icon span {\n    color: var(--cold);\n    font-weight: 800;\n}\n\n.gap {\n    width: 10px;\n    height: 16px;\n}\n\n.menu,\n.game {\n    width: 100vw;\n    height: 100vh;\n    display: flex;\n}\n\n.game {\n    align-items: center;\n    justify-content: center;\n    gap: 5px;\n}\n\n.game-frame,\n.game .panel {\n    height: 300px;\n    overflow: hidden;\n    width: 350px;\n    border-radius: 6px;\n    border: 3px solid #ffffff0a;\n    position: relative;\n}\n\n.game-frame iframe {\n    border: 0;\n    width: 100%;\n    height: 100%;\n    border-radius: 6px;\n}\n\n.game-frame #gameThumb {\n    height: 100%;\n    border-radius: 6px;\n    filter: grayscale(0.1) blur(2px);\n    width: 100%;\n    position: absolute;\n    scale: 1.05;\n}\n\n.btns {\n    display: flex;\n    gap: 2px;\n}\n\n.game .panel {\n    width: 200px;\n    display: flex;\n    padding: 4px;\n    flex-direction: column;\n    gap: 2px;\n}\n\n.checkbox {\n    background-color: black;\n    width: 15px;\n    height: 15px;\n    border: 2px solid rgb(2, 69, 71);\n    border-radius: 4px;\n    cursor: default;\n}\n\nbutton {\n    border: 2px solid rgb(2, 69, 71);\n    border-radius: 4px;\n    background: #111;\n    font-family: Inter;\n    color: var(--hot);\n    gap: 2px;\n}\n\nbutton:hover,\n.checkbox:hover {\n    background-color: #111120;\n}\n\n.checkbox.checked {\n    background: #2085a3;\n}\n\n.checkbox-container {\n    display: flex;\n    align-items: center;\n    gap: 4px;\n}\n\n.line {\n    width: 100%;\n    height: 3px;\n    background: #ffffff0a;\n    margin: 8px 0px;\n}\n\n#gameAuthorImage {\n    height: 25px;\n    border-radius: 50%;\n}\n\n.yCenter {\n    display: flex;\n    gap: 3px;\n    align-items: center;\n}\n\n.games {\n    width: 100%;\n    height: 100%;\n    overflow-y: auto;\n    display: flex;\n    flex-direction: column;\n    gap: 2px;\n}\n\n::-webkit-scrollbar {\n    background: linear-gradient(to right, transparent 4px, #141c33 4px);\n    border-radius: 3px;\n    width: 12px;\n}\n\n::-webkit-scrollbar-thumb {\n    background: linear-gradient(to right, transparent 4px, #182241 6px);\n    border-radius: 3px;\n}\n\n.editor {\n    display: flex;\n    position: absolute;\n    width: calc(100vw - 40px);\n    height: calc(100vh - 40px);\n    top: 20px;\n    left: 20px;\n    border-radius: 6px;\n    overflow: visible;\n    background-color: #ffffff2a;\n    backdrop-filter: blur(80px);\n    align-items: center;\n    justify-content: center;\n}\n\n.editor .close {\n    position: absolute;\n    top: -15px;\n    right: -15px;\n    background: #00000070;\n    backdrop-filter: blur(20px);\n    border-radius: 50%;\n    z-index: 5;\n    width: 30px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    color: rgb(255, 96, 96);\n    height: 30px;\n}\n\n.editor>iframe {\n    width: calc(100% - 20px);\n    height: calc(100% - 20px);\n    border: 0;\n    border-radius: 6px;\n}\n\n.editor .close i {\n    transition: scale 0.3s ease;\n}\n\n.editor .close>i:active {\n    scale: 1.1;\n}\n\n.hidden {\n    display: none !important;\n}",
  "html": "<script src=\"https://unpkg.com/eruda@3.4.1/eruda.js\"></script>\n<script>eruda.init()</script>\n<div class=\"menu\">\n    <span id=\"top-left\">Boxocera Linux</span>\n    <span id=\"top-left-2\">Euh, c'est quoi le sigma?</span>\n    <div class=\"center\">\n        <div class=\"mid\">\n            <div class=\"pre\"></div>\n            <img src=\"https://github.com/GavinGoGaming/boxocera/blob/main/BoxLogo.png?raw=true\" class=\"l\">\n            <img src=\"https://github.com/GavinGoGaming/boxocera/blob/main/GavinLogo.png?raw=true\" class=\"s\">\n            <img src=\"https://github.com/GavinGoGaming/boxocera/blob/main/ShkrLogo.png?raw=true\" class=\"r\">\n            <div class=\"post\"></div>\n        </div>\n    </div>\n    <div class=\"bar\">\n        <span id=\"icon\"><i class=\"fa-solid fa-circle-play\"></i> <span>SEARCH</span></span>\n        <span id=\"icon\"><i class=\"fa-solid fa-circle-left\"></i> <span>BACK</span></span>\n        <span id=\"icon\"><i class=\"fa-solid fa-circle-up\"></i> <span>RANDOM</span></span>\n        <span id=\"icon\"><i class=\"fa-solid fa-left-right\"></i> <span>CHOOSE</span></span>\n        <span id=\"icon\"><i class=\"fa-solid fa-circle-down\"></i> <span>SELECT</span></span>\n        <span id=\"icon\"><i class=\"fa-solid fa-files\"></i> <span>FILES</span></span>\n    </div>\n    \n    <div class=\"warn\">\n        <h1><i class=\"fa-solid fa-sharp fa-mouse-pointer\"></i> Mouse Not Supported</h1>\n        <span>Boxocera for Web is developed to support arrow keys.</span>\n        <span id=\"icons\"><i class=\"fa-solid fa-square-up\"></i><i class=\"fa-solid fa-square-down\"></i><i class=\"fa-solid fa-square-left\"></i><i class=\"fa-solid fa-square-right\"></i></span>\n        <div class=\"gap\"></div>\n        <span><i>Wait for this message to exit</i></span>\n    </div>\n</div>\n\n<div class=\"game hidden\">\n    <div class=\"game-frame\">\n        <img src=\"\" id=\"gameThumb\">\n        <iframe src=\"\"></iframe>\n    </div>\n    <div class=\"panel\">\n        <b id=\"gameName\">Box World Tour</b>\n        <span class=\"yCenter\"><img src=\"\" id=\"gameAuthorImage\"> <span id=\"gameAuthor\"></span></span>\n        <div class=\"line\"></div>\n        <div class=\"checkbox-container\">\n            <div id=\"interpolate\" class=\"checkbox checked\"></div>\n            <span>Interpolation</span>\n        </div>\n        <div class=\"btns\">\n            <button id=\"reload\"><i class=\"fa-solid fa-refresh\"></i></button>\n            <button id=\"closepage\"><i class=\"fa-solid x\"></i></button>\n            <button id=\"link\"><i class=\"fa-solid fa-arrow-up-right-from-square\"></i></button>\n            <button id=\"big\"><i class=\"fa-solid fa-expand\"></i> Big</button>\n        </div>\n        <div class=\"line\"></div>\n        <div class=\"games\"></div>\n    </div>\n</div>\n<div class=\"editor hidden\">\n    <div class=\"close\"><i class=\"fa-solid fa-circle-x\"></i></div>\n    <iframe id=\"editorFrame\" src=\"\"></iframe>\n</div>",
